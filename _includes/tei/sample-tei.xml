	<div xml:id="poem2">
		<text xml:id="transcription">
			<pb facs="#f001" />
			<lg>
				<l>yo no chōni</l>
				<l>sasameki tsukishi</l>
				<l>hoshi no ima wo</l>
				<l>gekai no hito no</l>
				<l>bin no hotsure yo</l>
			</lg>
		</text>
		<text xml:id="translation">
			<pb facs="#f001" />
			<lg>
				<l>bedroom curtains</l>
				<l>the lovers’ talk ends</l>
				<l>now a star</l>
				<l>one person of this earth</l>
				<l>has stray hairs at the temple</l>
			</lg>
		</text>
	</div>
